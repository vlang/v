{
    "__comment": "Based on https://github.com/python/cpython/blob/main/.github/problem-matchers/gcc.json",
    "problemMatcher": [
        {
            "owner": "v-problem-matcher",
            "pattern": [
                {
                    "regexp": "^(.*):(\\d+):(\\d+):\\s+(?:fatal\\s+)?(warning|error|notice):\\s+(.*)$",
                    "file": 1,
                    "line": 2,
                    "column": 3,
                    "severity": 4,
                    "message": 5
                }
            ]
        }
    ]
}
