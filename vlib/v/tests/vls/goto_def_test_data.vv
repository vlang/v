	println(another.value)

	yet_another := obj
	println(yet_another)

	imported_e := s.PublicEnum1.a
	println(imported_e)

	val := obj.value
	println(val)

	deep := DeepStruct{}
	nested_val := deep.st.sstt.l2
	println(nested_val)

	embedded_val := deep.LocalStruct
	println(embedded_val)

	result := obj.my_method()
	println(result)

	ret_type := obj.my_method()
	println(ret_type)
