	_ := GenericSample[u8]{256}
	_ := GenericSample[u16]{65536}
	_ := GenericSample[u32]{4294967296}

	_ := GenericSample[i8]{128}
	_ := GenericSample[i8]{-129}
	_ := GenericSample[i16]{32768}
