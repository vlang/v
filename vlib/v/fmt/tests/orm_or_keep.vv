import sqlite

struct User {
	id   i64    [primary; sql: serial]
	name string [unique]
}

fn main() {
	db := sqlite.connect(':memory:')?
	sql db {
		create table User
	} or { panic(err) }
	sql db {
		insert user into User
	} or {
		println('user should have been inserted, but could not, err: $err')
		exit(1)
	}
}
