interface ITest {
	a MySum
}

struct Test {
	a MySum
}

type MySum = bool | int

fn interface_var(a ITest) {
	match a {
		Test {
			$dbg;
		}
		else {}
	}
}

struct MyTest {
	t ITest
}

fn main() {
	interface_var(Test{ a: true })
	t1 := Test{}
	t2 := MyTest{
		t: t1
	}
	mut t3 := Test{}
	t4 := MyTest{
		t: t3
	}
	$dbg;
}
