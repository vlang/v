module main

enum Color {
	orange
	green  = 5
	red    = 2
	yellow = 3
}

struct Aaa {
	color Color
}

fn main() {
	col := Color.green
	a := Aaa{
		color: col
	}
	orange := Color.orange
	println(orange)
	println(Color.yellow)
	println(col)
	println(a.color)
	println('interp: ${col}')
	println('interp: ${a.color}')
	println((Color.orange))
	println([Color.orange, Color.green][0])
}
