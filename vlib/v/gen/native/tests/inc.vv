fn signed_inc_test() {
	mut a := i64(1)
	println(a)
	a++
	println(a)
	mut b := i32(1)
	println(b)
	b++
	println(b)
	mut c := i16(1)
	println(c)
	c++
	println(c)
	mut d := i8(1)
	println(d)
	d++
	println(d)
	mut e := int(1)
	println(e)
	e++
	println(e)

	mut f := i64(-1)
	println(f)
	f++
	println(f)
	mut g := i32(-1)
	println(g)
	g++
	println(g)
	mut h := i16(-1)
	println(h)
	h++
	println(h)
	mut i := i8(-1)
	println(i)
	i++
	println(i)
	mut j := int(-1)
	println(j)
	j++
	println(j)
}

fn unsigned_inc_test() {
	mut f := u64(1)
	println(f)
	f++
	println(f)
	mut g := u32(1)
	println(g)
	g++
	println(g)
	mut h := u16(1)
	println(h)
	h++
	println(h)
	mut i := u8(1)
	println(i)
	i++
	println(i)
}

fn main() {
	signed_inc_test()
	unsigned_inc_test()
}
