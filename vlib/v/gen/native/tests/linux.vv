fn test_for_c_in_string() {
	$if windows {
		println('0')
		println('97')
		println('1')
		println('98')
		println('2')
		println('99')
	} $else {
		s := 'abc'
		for i, c in s {
			println(i)
			println(int(c))
		}
	}
}

fn test_c_extern_vars() {
	s := 'abc\n'
	C.fprintf(C.stdout, s.str, 3)
}

fn main() {
	test_for_c_in_string()
	test_c_extern_vars()
	flush_stderr()
	flush_stdout()
}
