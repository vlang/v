fn main() {
	test_int()
	test_fp()
	test_unsafe()
	test_alias(100, 9)
}

fn test_int() {
	a := 100
	mut b := a
	b += b
	b += 50

	assert b == 250

	mut c := u8(b)
	d := 4
	c += u8(-d)
	println(c)
}

fn test_fp() {
	a := 1.0
	mut b := a
	b += 0.5
	b *= 4
	b /= 2

	println(int(b))
}

fn test_unsafe() {
	a := 10

	unsafe {
		b := 4
		println(a + b)
	}
}

type Integer = int

fn test_alias(a Integer, b Integer) {
	e := a + b
	assert e == a + b
	println(int(e))
}
