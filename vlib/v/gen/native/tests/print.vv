struct Foo {
	a int
	b int
	c int
}

fn test_stdout() {
	print('Hello ')
	println('World')
}

fn test_booleans() {
	println(true)
	println(false)
}

fn test_numbers() {
	println(123)
}

fn test_oof() {
	println(__offsetof(Foo, a))
	println(__offsetof(Foo, b))
	println(__offsetof(Foo, c))
}

fn test_stderr() {
	eprint('2(Hello)')
	eprintln('2(World)')
}

fn test_idents() {
	x := 0
	println(x)

	y := 5
	println(y)

	z := -8
	println(z)

	a := 123
	println(a)

	b := -456
	println(b)
}

fn main() {
	test_stdout()
	test_stderr()
	test_numbers()
	test_oof()
	test_idents()
}
